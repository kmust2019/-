package com.company;

import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        boolean found=false;
        int[] data ={2,4,5,4,7,8,9,45};
        int x=in.nextInt();
        int loc=-1;
        for (int i=0;i<data.length;i++)
        {
            if (x==data[i])
            {
                loc=i;
                break;
            }
        }
        for (int k:data)
        {
            if (k==x)
            {
                found=true;
            }
        }
        if (loc>-1)
        {
            System.out.println(x + "是第" + (loc + 1) + "个");
        }
        else
        {
            System.out.println(x + "不在其中" );
        }
    }
}
